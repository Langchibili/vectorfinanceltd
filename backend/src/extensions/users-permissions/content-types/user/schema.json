{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "profilePicture": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "details": {
      "displayName": "details",
      "type": "component",
      "repeatable": false,
      "component": "user-profile.details"
    },
    "clientDetails": {
      "displayName": "clientDetails",
      "type": "component",
      "repeatable": false,
      "component": "user-profile.client-details"
    },
    "currentLoan": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::loan.loan"
    },
    "fulfilledLoans": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::loan.loan"
    },
    "repayment": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::repayment.repayment",
      "mappedBy": "client"
    },
    "loans": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::loan.loan",
      "mappedBy": "client"
    },
    "approvals": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::approval.approval",
      "mappedBy": "client"
    },
    "transactionHistories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::transaction-history.transaction-history",
      "mappedBy": "client"
    },
    "fullnames": {
      "type": "string"
    },
    "salary": {
      "type": "component",
      "repeatable": false,
      "component": "client-details.salary"
    },
    "applicationForms": {
      "displayName": "application-forms",
      "type": "component",
      "repeatable": true,
      "component": "forms.application-forms"
    },
    "formsToFill": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::form.form",
      "inversedBy": "clientsWhoCanFill"
    },
    "activities": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::notification.notification",
      "mappedBy": "notifier"
    },
    "notifications": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::notification.notification",
      "mappedBy": "notifiedUsers"
    },
    "business": {
      "displayName": "business",
      "type": "component",
      "repeatable": false,
      "component": "client-details.business"
    },
    "currentInvestment": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::investment.investment"
    },
    "investments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::investment.investment",
      "mappedBy": "client"
    },
    "signature": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "signingMethod": {
      "type": "enumeration",
      "enum": [
        "e-signing",
        "manual"
      ],
      "default": "e-signing"
    },
    "witnessSignature": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false
    }
  }
}
