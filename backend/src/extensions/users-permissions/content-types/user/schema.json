{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "profilePicture": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "details": {
      "type": "component",
      "repeatable": false,
      "component": "user-profile.details"
    },
    "clientDetails": {
      "type": "component",
      "repeatable": true,
      "component": "user-profile.client-details"
    },
    "pendingLoans": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::loan.loan"
    },
    "fulfilledLoans": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::loan.loan"
    },
    "repayment": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::repayment.repayment",
      "mappedBy": "client"
    },
    "loans": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::loan.loan",
      "mappedBy": "client"
    },
    "approvals": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::approval.approval",
      "mappedBy": "client"
    },
    "transactionHistories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::transaction-history.transaction-history",
      "mappedBy": "client"
    },
    "ky_cverification": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::ky-cverification.ky-cverification",
      "mappedBy": "client"
    }
  }
}
